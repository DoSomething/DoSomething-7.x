<?php

/**
 * @file
 * Provides a devel tab for webform submissions.
 */

/**
 * Implements hook_menu().
 */
function webform_devel_menu() {
  $items['node/%webform_menu/submission/%webform_menu_submission/devel'] = array(
    'title' => 'Devel',
    'page callback' => 'webform_devel_load_object',
    'page arguments' => array(1, 3),
    'access arguments' => array('access devel information'),
    'type' => MENU_LOCAL_TASK,
    'weight' => 100,
  );
  return $items;
}

function webform_devel_load_object($node, $submission) {
  module_load_include('inc', 'webform', 'includes/webform.submissions');
  return kdevel_print_object($submission, '$submission->');
}
